/**index.wxss**/
.container{
  width: 100%;
  height: 100%;
  background: #f4f4f4;
  position: relative;
}
.nav{
  width: 100%;
  height: 50rpx;
  background: #fff;
  display: flex;
}
.nav view{
  flex: 1;
  text-align: center;
  font-size: 24rpx;
  line-height: 50rpx;
}
.box{
  width: 85%;
  min-height:300rpx;
  background: #fff;
  margin: 50rpx auto;
  padding: 10rpx 0;
}
.h3{
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 10rpx;
}
.li{
  width: 85%;
  background: #f4f4f4;
  margin: 40rpx auto;
  line-height: 70rpx;
  font-size: 24rpx;
  text-align: center;
}
.flxd{
  position: absolute;
  bottom: 100rpx;
  width: 80rpx;
  height: 80rpx;
  border: 1rpx solid #ccc;
  text-align: center;
  line-height: 80rpx;
  border-radius: 40rpx;
  font-size: 80rpx;
  color: rgb(160, 156, 156);
  left: 40%;
}